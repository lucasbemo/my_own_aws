{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Sid": "DynamoDBPutItem",
        "Effect": "Allow",
        "Action": [
          "dynamodb:DescribeTable",
          "dynamodb:Query",
          "dynamodb:PutItem"
        ],
        "Resource": [
          "arn:aws:dynamodb:us-east-1:385680495520:table/lab_dynamodb"
        ]
      },
      {
        "Sid": "cloudwatchlog",
        "Effect": "Allow",
        "Action": [
          "logs:CreateLogGroup",
          "logs:CreateLogStream",
          "logs:DescribeLogGroups",
          "logs:DescribeLogStreams",
          "logs:PutLogEvents",
          "logs:GetLogEvents",
          "logs:FilterLogEvents"
        ],
        "Resource": "*"
      }
    ]
  }